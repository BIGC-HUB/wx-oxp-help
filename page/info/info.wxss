page {
    background-color: #efeff4;
}
.info_top{
    background: white;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;
    display: flex;
    height: 106rpx;
    justify-content: space-between;
}
.info_bottom{
    padding: 0 30rpx;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;
    background: white;
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 88rpx;
}
.info_bottom text, .info_bottom image {
    pointer-events: none;
}
.team_phone {
    display: inline-flex;
    justify-content: space-between;
    width: calc(100% - 48rpx);
}
.empty {
  height: 44rpx;
  background-color: #efeff4;
  width: 100%;
}
.call {
  height: 48rpx;
  width: 48rpx;
}
.icon {
  height: 48rpx;
  width: 48rpx
}
.icon_bg {
  height:80rpx;
  width: 300rpx;
  margin-top: 14rpx;
  margin-left: 38rpx;
}
.team_name {
    /* 禁止 换行 */
    white-space: nowrap;
    /* 隐藏 超出部分 */
    overflow: hidden;
    /* 省略号 替代 */
    text-overflow: ellipsis;
    margin-top: 34rpx;
    margin-bottom: 28rpx;
    margin-right: 30rpx;
}
text{
  font-size: 34rpx;
}
